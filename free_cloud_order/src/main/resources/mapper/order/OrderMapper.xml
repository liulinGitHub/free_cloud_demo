<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.cloud.face.dao.OrderMapper">

    <select id="selectOrder" resultType="org.cloud.face.entity.Order">
        SELECT
            order_id orderId,
            order_code orderCode
        FROM
            order_master
    </select>
</mapper>
